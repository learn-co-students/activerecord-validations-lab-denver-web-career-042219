<h1>Create an Author</h1>
<% if @author.errors.any?  %>
  <ul>
    <% @author.errors.full_messages.each do |msg| %>
    <li><%= msg %></li>
    <% end %>
  </ul>
<% end %>
<%= form_for :author, url:authors_path do |f| %>
    <%= f.label :name %><br>
    <%= f.text_field :name %><br/>
    <%= f.label :phone_number %><br>
    <%= f.text_field :phone_number %><br/><br/>
  <%= f.submit %>
<% end %>
